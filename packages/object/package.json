{
  "name": "@pixpilot/object",
  "type": "module",
  "version": "1.2.0",
  "packageManager": "pnpm@10.17.0",
  "description": "A collection of utility functions for object manipulation and transformation.",
  "author": "Pixpilot <m.doaie@hotmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixpilot/js-utils.git",
    "directory": "packages/object"
  },
  "keywords": [],
  "exports": "./src/index.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "clean": "git clean -xdf .cache .turbo dist",
    "clean:all": "git clean -xdf .cache .turbo dist node_modules",
    "build": "tsdown",
    "buld:watch": "tsdown --watch",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore"
  },
  "dependencies": {
    "@fastify/deepmerge": "^3.1.0",
    "@pixpilot/string": "workspace:*",
    "dot-prop": "^10.1.0"
  },
  "devDependencies": {
    "@internal/eslint-config": "workspace:*",
    "@internal/prettier-config": "workspace:*",
    "@internal/tsconfig": "workspace:*",
    "@internal/tsdown-config": "workspace:*",
    "@internal/vitest-config": "workspace:*",
    "@manypkg/get-packages": "^3.1.0",
    "@types/node": "catalog:dev",
    "eslint": "catalog:dev",
    "tsdown": "catalog:dev",
    "typescript": "catalog:dev"
  },
  "prettier": "@internal/prettier-config"
}
